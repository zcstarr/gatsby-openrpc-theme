{
  "name": "open-rpc/gatsby-openrpc-theme",
  "description": "A gatsby theme for generated OpenRPC documentation",
  "version": "0.0.0-development",
  "publishConfig": {
    "access": "public"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
